/* 全局引入iconfont样式 */
@import "./style/iconfont.wxss";

/* 初始化全局样式 */
page, view, text, swiper, swiper-item, image, navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 全局变量定义，设置全局主题颜色 */
page {
  /* 主题颜色 */
  --themeColor: #ff5700;
  /* 字体颜色大小 rpx自适应大小 */
  font-size: 28rpx;
}

image {
  width: 100%;
}