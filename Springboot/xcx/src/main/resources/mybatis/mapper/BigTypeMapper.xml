<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xcx.mapper.BigTypeMapper">

    <resultMap id="bigTypeResult" type="com.xcx.entity.BigType"></resultMap>

    <select id="findById" parameterType="Integer" resultMap="bigTypeResult">
        select * from t_bigType where id=#{id}
    </select>
</mapper>